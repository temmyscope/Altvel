{
    "name": "altvel/altvel",
    "description": "High Speed PHP Web Application Framework For PHP Engineers.",
    "type": "project",
    "version": "v0.8.9",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.3.0",
        "jenssegers/blade": "^1.3.0",
        "monolog/monolog": "^2.0@dev",
        "php-di/php-di": "^6.0.11",
        "sevens/consoler": "^0.1.0",
        "sevens/model-trait": "^1.0.0",
        "sevens/router": "^1.0.0",
        "sevens/uploader-trait": "^1.0.0",
        "sevens/vars": "^1.0.0",
        "symfony/http-foundation": "^5.2",
        "vlucas/phpdotenv": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Providers\\": "app/Providers/",
            "App\\Controllers\\": "app/Controllers/"
        },
        "files": [
            "app/Helper/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Elisha Temiloluwa a.k.a TemmyScope",
            "email": "temmyscope@protonmail.com",
            "role": "Developer"
        }
    ],
    "scripts": {
        "post-create-project-cmd": [
            "@php Engineer app::start"
        ]
    },
    "keywords": [
        "altvel", "engineer",
        "laravel", "framework",
        "engineer"
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5@dev"
    }
}
